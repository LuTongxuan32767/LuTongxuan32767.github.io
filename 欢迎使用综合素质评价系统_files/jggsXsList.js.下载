if(window.jggsXsList){alert("jggsXsList 冲突了！")}else{window.jggsXsList={}}$.extend(jggsXsList,{grid:null,ajaxType:"get",columnDatas:[],queryData:function(){if(jggsXsList.grid){jggsXsList.isSentAjaxFlag=true;jggsXsList.grid.fnClearTable(0);jggsXsList.grid.fnDraw()}},resetData:function(){$("#jggsxsXm").val("");$("#jggsxjfh").val("")},isSentAjaxFlag:false});$(function(){jggsXsList.columnDatas.push({data:null,title:"序号",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(d,b,e,c){return c.row+1}});jggsXsList.columnDatas.push({data:"xxmc",title:"学校名称",bSortable:false,bVisible:true,sClass:"text-center"});jggsXsList.columnDatas.push({data:"njmc",title:"年级",bSortable:false,bVisible:true,sClass:"text-center"});jggsXsList.columnDatas.push({data:"bjmc",title:"班级",bSortable:false,bVisible:true,sClass:"text-center"});jggsXsList.columnDatas.push({data:"xm",title:"学生姓名",bSortable:false,bVisible:true,sClass:"text-center"});jggsXsList.columnDatas.push({data:"xjfh",title:"省学籍辅号",bSortable:false,bVisible:true,sClass:"text-center"});jggsXsList.columnDatas.push({data:"xsJbxxId",title:"操作",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(d,b,e,c){return"<a href='javascript:void(0);' onclick='jggsListWgt.queryGsDack(\""+d+"\")'>查看档案</a>"}});var a={sAjaxSource:jggsXsListConst.jggsXsAjaxUrl,bServerSide:true,columns:jggsXsList.columnDatas,ajaxType:jggsXsList.ajaxType,getParam:function(b,c,e,d){return{method:"queryJggsXs",fbszId:jggsXsListConst.fbszId,gsfbGsfw:jggsXsListConst.gsfbGsfw,xm:encodeURI($("#jggsxsXm").val()),xjfh:$("#jggsxjfh").val()}},beforeAjax:function(){return jggsXsList.isSentAjaxFlag}};jggsXsList.grid=$("#jggsXsListGrid").e5dataTable(a);$("#jggsqueryXsList").click(function(){jggsXsList.isSentAjaxFlag=true;jggsXsList.queryData()});$("#myModalXs").on("hidden.bs.modal",function(){jggsXsList.resetData()});$("#btn-reset").click(function(){jggsXsList.resetData()})});
if (window.xsXqda) {
	alert("xsXqda 冲突了！");
} else {
	window.xsXqda = {};
}

$.extend(xsXqda, {
	//Knockout数据绑定
	obj : {
		xsDaRzList:ko.observableArray([]),//任职情况
		xsDaJcList:ko.observableArray([]),//奖惩情况
		xsJbxxId : "",
	},
	str : null,//定义拼接字符串变量
	//查询字典信息
	queryXsXqda : function() {
		//查询信息
		$.post(xsXqDajbqkConst.queryAjaxUrl, {method: "queryXsXqda",xsJbxxId:xsXqda.xsJbxxId,xn:xsXqDajbqkConst.xn,xq:xsXqDajbqkConst.xq}, function(data) {
			if(data!=null&&data!=""){
				xsXqda.obj.xsDaRzList(data.rzList);
				if(data.jcList!=null&&data.jcList!=undefined){
				for(var i=0;i<data.jcList.length;i++){
					/*data.jcList[i].xn = xnxqqk.getxnxqmc(data.jcList[i].xn);*/
					data.jcList[i].xq = CommonDict.get("DIC_ZD_XT_XQMC", data.jcList[i].xq);
					var jcdj = data.jcList[i].jcqk_jcdj;
					/*data.jcList[i].jcqk_jcdj = CommonDict.get("DIC_ZD_XT_JCDJ", jcdj);*/
					if(jcdj == 1){
						data.jcList[i].jcqk_jcdj = "校级";
					}else if(jcdj == 2){
						data.jcList[i].jcqk_jcdj = "县级";
					}else if(jcdj == 3){
						data.jcList[i].jcqk_jcdj = "市级";
					}else if(jcdj == 4){
						data.jcList[i].jcqk_jcdj = "省级";
					}else if(jcdj == 5){
						data.jcList[i].jcqk_jcdj = "国家级";
					}else if(jcdj == 6){
						data.jcList[i].jcqk_jcdj = "国际级";
					}
				 }
				}
				xsXqda.obj.xsDaJcList(data.jcList);
			}
		}).error(function(msg) {
			E5.Msg.alert("信息查询失败！");
		});
	}
	
	

});

$(function(){
	//绑定Knockout
	var koxsXqda = document.getElementById("xsXqdaKo");
	if (koxsXqda)
	{
		ko.applyBindings(xsXqda.obj, koxsXqda);
	}
});

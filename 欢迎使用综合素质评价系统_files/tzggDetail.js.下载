if(window.tzggDetail){alert("tzggDetail 冲突了！")}else{window.tzggDetail={}}$.extend(tzggDetail,{str:null,obj:{tzbt:ko.observable(""),fbrmc:ko.observable(""),fbsj:ko.observable(""),rznr:ko.observable("")},queryFjbByTztgId:function(a){$.post(tzggConst.tzggAjaxUrl,{method:"queryFjbByTztgId",tztg:a},function(f){var l=f.rows;if(l&&l.length>0){for(var g=0;g<l.length;g++){var k=l[g];var d=k.fjmc;var c=k.fjlj;var b=k.pid;var m=c;var e=d;var h="../../upload"+m.substring(m.lastIndexOf("\\"));var j=m.substring(m.lastIndexOf(".")+1);if(j=="jpg"||j=="png"||j=="jpeg"||j=="gif"||j=="bmp"){tzggDetail.str='<div class="pic_show"><a class="filecz lightsGal" fileId="'+b+'" fileType="1" istztg="1">'+e+"</a></div>"}if(j=="txt"||j=="xlsx"||j=="xls"||j=="docx"||j=="doc"||j=="rar"||j=="zip"||j=="ppt"||j=="pptx"||j=="html"){tzggDetail.str='<div class="pic_show"><a class="filecz xiazai" fileId="'+b+'" fileType="1" istztg="1" >'+e+"</a></div>"}$("#fileAddrDetail1").append(tzggDetail.str)}}}).error(function(b){E5.Msg.alert("附件信息查询失败！")})},queryByIdFromIndex:function(b,a){$.post(tzggConst.tzggAjaxUrl,{method:"queryById",pid:b},function(c){ydlsIndex=a;tzggDetail.obj.tzbt(c.tzbt);tzggDetail.obj.fbrmc(c.fbrmc);tzggDetail.obj.fbsj(c.fbsj);tzggDetail.obj.rznr(c.rznr);tzggDetail.queryFjbByTztgId(b);tzggDetail.saveYdls(b)}).error(function(c){E5.Msg.alert("信息查询失败！")})},saveYdls:function(a){$.post(tzggConst.tzggAjaxUrl,{method:"saveYdlsByTztgId",tztg:a},function(b){}).error(function(b){E5.Msg.alert("信息查询失败！")})},reLoadAndClear:function(){tzggDetail.obj.tzbt("");tzggDetail.obj.fbrmc("");tzggDetail.obj.fbsj("");tzggDetail.obj.rznr("");$("#fileAddrDetail1").html("");tzggBt.queryData(1)}});$(function(){var a=document.getElementById("koTzggDetail");if(a){ko.applyBindings(tzggDetail.obj,a)}$("#detailModal").on("hidden.bs.modal",function(){tzggDetail.reLoadAndClear()})});
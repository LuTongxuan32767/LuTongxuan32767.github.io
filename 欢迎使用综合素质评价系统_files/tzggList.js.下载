if(window.tzggList){alert("tzggList 冲突了！")}else{window.tzggList={}}$.extend(tzggList,{grid:null,ajaxType:"post",columnDatas:[],queryData:function(){if(tzggList.grid){tzggList.isSentAjaxFlag=true;tzggList.grid.fnClearTable(0);tzggList.grid.fnDraw()}},isSentAjaxFlag:false});$(function(){tzggList.columnDatas.push({data:null,title:"序号",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(d,b,e,c){return c.row+1}});tzggList.columnDatas.push({data:"tzfl",title:"类型",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(c,b,d){return CommonDict.get("DIC_ZD_TZTG_LX",d.tzfl)}});tzggList.columnDatas.push({data:"tzbt",title:"标题",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(c,b,d){return"<a href='javascript:void(0);' data-toggle='modal' data-target='#detailModal' onclick='tzggDetail.queryByIdFromIndex(\""+d.pid+'","'+d.ydls+"\")'>"+c+"</a>"}});tzggList.columnDatas.push({data:"fbrmc",title:"发布单位",bSortable:false,bVisible:true,sClass:"text-center"});tzggList.columnDatas.push({data:"fbsj",title:"发布时间",bSortable:false,bVisible:true,sClass:"text-center"});var a={sAjaxSource:tzggConst.tzggAjaxUrl,bServerSide:true,ajaxType:tzggList.ajaxType,columns:tzggList.columnDatas,getParam:function(b,c,e,d){return{method:"queryTitle"}},beforeAjax:function(){return tzggList.isSentAjaxFlag}};tzggList.grid=$("#tzggListGrid").e5dataTable(a)});
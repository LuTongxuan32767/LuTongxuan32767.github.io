if(window.tzggBt){alert("tzggBt 冲突了！")}else{window.tzggBt={}}$.extend(tzggBt,{obj:{content:ko.observableArray([])},queryData:function(a){$.post(tzggConst.tzggAjaxUrl,{method:"queryTitle",pageSize:5},function(b){if(b.totalCount<1){$("#content").remove()}else{$("#removeContent").remove();tzggBt.obj.content.removeAll();$.each(b.rows,function(d,e){if(e.pid!=null){var c="";if(e.tzbt.length>18){c=e.tzbt.substr(0,18)+"..."}else{c=e.tzbt}tzggBt.obj.content.push({pid:e.pid,tzbt:c,ytzbt:e.tzbt,fbsj:e.fbsj,ydls:e.ydls})}})}if(a==1){tzggList.queryData()}$(".tzggnr").each(function(){var e=new Array();e=$(this).attr("id").split("-");var d=$(this).attr("id");var c=e[0];var f=e[1];var h=$("#"+c).text();var g="";$("#"+c+"-"+f).css({"max-width":"60%","text-align":"left"});$.post(tzggConst.tzggAjaxUrl,{method:"queryDqdlr",},function(i){g=i.dqdlr;$("#"+d).on("click",function(){tzggDetail.queryByIdFromIndex(c,f)});if(g=="1"){$("#"+d).text(h);$("#"+d).on("mouseover",function(){$("#"+c+"-"+f).css({"max-width":"100%","text-align":"left"})});$("#"+d).on("mouseout",function(){$("#"+c+"-"+f).css({"max-width":"60%","text-align":"left"})})}else{$("#"+d).on("mouseover",function(){if(h.length>18){$("#"+c+f).hide();$("#"+c+"-"+f).css({"max-width":"100%","text-align":"left"})}$("#"+d).text(h)});$("#"+d).on("mouseout",function(){var j=h;$("#"+c+f).show();$("#"+c+"-"+f).css({"max-width":"60%","text-align":"left"});document.getElementById(d).style.property="";$("#"+d).text(j)})}})})}).error(function(b){E5.Msg.alert("信息查询失败！")})}});$(function(){var a=document.getElementById("koTzggBt");if(a){ko.applyBindings(tzggBt.obj,a)}});
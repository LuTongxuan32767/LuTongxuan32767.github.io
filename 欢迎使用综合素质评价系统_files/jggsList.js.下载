if(window.jggsList){alert("jggsList 冲突了！")}else{window.jggsList={}}$.extend(jggsList,{grid:null,columnDatas:[],queryData:function(){if(jggsList.grid){jggsList.isSentAjaxFlag=true;jggsList.grid.fnClearTable(0);jggsList.grid.fnDraw()}},isSentAjaxFlag:false});$(function(){jggsList.columnDatas.push({data:null,title:"序号",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(d,b,e,c){return c.row+1}});jggsList.columnDatas.push({data:"gsfbGsbt",title:"公示标题",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(d,c,g){var e=g.fbszId;var f=g.gsfbGsfw;var b=g.gsfbGsfs;return"<a href='javascript:void(0)' onclick='jggsListWgt.clickjggs(\""+e+'", "'+f+'", "'+b+"\")'>"+d+"</a>"}});jggsList.columnDatas.push({data:"gsfbGsfw",title:"公示范围",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(c,b,d){return CommonDict.get("DIC_ZD_XT_FBFS",c)}});jggsList.columnDatas.push({data:"gsfbGsfs",title:"公示方式",bSortable:false,bVisible:true,sClass:"text-center"});jggsList.columnDatas.push({data:"gsfbStart",title:"开始时间",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(c,b,d){return c.substr(0,4)+"/"+c.substr(4,2)+"/"+c.substr(6)}});jggsList.columnDatas.push({data:"gsfbEnd",title:"结束时间",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(c,b,d){return c.substr(0,4)+"/"+c.substr(4,2)+"/"+c.substr(6)}});var a={sAjaxSource:jggsListConst.jggsListAjaxUrl,bServerSide:true,columns:jggsList.columnDatas,getParam:function(b,c,e,d){return{method:"queryJggsMoreList"}},beforeAjax:function(){return jggsList.isSentAjaxFlag}};jggsList.grid=$("#jggsListGrid").e5dataTable(a)});
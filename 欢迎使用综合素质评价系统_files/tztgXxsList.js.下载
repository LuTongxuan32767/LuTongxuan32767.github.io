if(window.tztgXxsList){alert("tztgXxsList 冲突了！")}else{window.tztgXxsList={}}$.extend(tztgXxsList,{grid:null,columnDatas:[],queryData:function(){if(tztgXxsList.grid){tztgXxsList.isSentAjaxFlag=true;tztgXxsList.grid.fnClearTable(0);tztgXxsList.grid.fnDraw()}},isSentAjaxFlag:false});$(function(){tztgXxsList.columnDatas.push({data:null,title:"序号",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(d,b,e,c){return c.row+1}});tztgXxsList.columnDatas.push({data:"tzfl",title:"类型",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(c,b,d){return CommonDict.get("DIC_ZD_TZTG_LX",d.tzfl)}});tztgXxsList.columnDatas.push({data:"tzbt",title:"标题",bSortable:false,bVisible:true,sClass:"text-center",mRender:function(c,b,d){return"<a href='javascript:void(0);' data-toggle='modal' data-target='#tztgXxsDetailModal' onclick='tztgXxsDetail.queryByIdFromIndex(\""+d.pid+'","'+d.ydls+"\")'>"+c+"</a>"}});tztgXxsList.columnDatas.push({data:"fbrmc",title:"发布单位",bSortable:false,bVisible:true,sClass:"text-center"});tztgXxsList.columnDatas.push({data:"fbsj",title:"发布时间",bSortable:false,bVisible:true,sClass:"text-center"});var a={sAjaxSource:headConst.tztgAjaxUrl,bServerSide:true,columns:tztgXxsList.columnDatas,getParam:function(b,c,e,d){return{method:"queryTztgXxs"}},beforeAjax:function(){return tztgXxsList.isSentAjaxFlag}};tztgXxsList.grid=$("#tztgXxsListGrid").e5dataTable(a)});
if(window.tztgXxsDetail){alert("tztgXxsDetail 冲突了！")}else{window.tztgXxsDetail={}}$.extend(tztgXxsDetail,{str:null,obj:{tzbt:ko.observable(""),fbrmc:ko.observable(""),fbsj:ko.observable(""),rznr:ko.observable("")},queryFjbByTztgId:function(a){$.post(headConst.tztgAjaxUrl,{method:"queryFjbByTztgId",tztg:a},function(f){var l=f.rows;if(l&&l.length>0){for(var g=0;g<l.length;g++){var k=l[g];var d=k.fjmc;var c=k.fjlj;var b=k.pid;var m=c;var e=d;var h="../../upload"+m.substring(m.lastIndexOf("\\"));var j=m.substring(m.lastIndexOf(".")+1);if(j=="jpg"||j=="png"||j=="jpeg"||j=="gif"||j=="bmp"){tztgXxsDetail.str='<div class="pic_show"><a class="filecz lightsGal" fileId="'+b+'" fileType="1" istztg="1">'+e+"</a></div>"}if(j=="txt"||j=="xlsx"||j=="xls"||j=="docx"||j=="doc"||j=="rar"||j=="zip"||j=="ppt"||j=="pptx"||j=="html"){tztgXxsDetail.str='<div class="pic_show"><a class="filecz xiazai" fileId="'+b+'" fileType="1" istztg="1" >'+e+"</a></div>"}$("#fileAddrDetail").append(tztgXxsDetail.str)}}}).error(function(b){E5.Msg.alert("附件信息查询失败！")})},queryByIdFromIndex:function(b,a){$.post(headConst.tztgAjaxUrl,{method:"queryById",pid:b},function(c){ydlsIndex=a;tztgXxsDetail.obj.tzbt(c.tzbt);tztgXxsDetail.obj.fbrmc(c.fbrmc);tztgXxsDetail.obj.fbsj(c.fbsj);tztgXxsDetail.obj.rznr(c.rznr);tztgXxsDetail.queryFjbByTztgId(b);tztgXxsDetail.saveYdls(b)}).error(function(c){E5.Msg.alert("信息查询失败！")})},saveYdls:function(a){$.post(headConst.tztgAjaxUrl,{method:"saveYdlsByTztgId",tztg:a},function(b){}).error(function(b){E5.Msg.alert("信息查询失败！")})},reLoadAndClear:function(){tztgXxsDetail.obj.tzbt("");tztgXxsDetail.obj.fbrmc("");tztgXxsDetail.obj.fbsj("");tztgXxsDetail.obj.rznr("");$("#fileAddrDetail").html("");tztgXxsList.queryData()}});$(function(){var a=document.getElementById("koTztgXxsDetail");if(a){ko.applyBindings(tztgXxsDetail.obj,a)}$("#tztgXxsDetailModal").on("hidden.bs.modal",function(){tztgXxsDetail.reLoadAndClear()})});
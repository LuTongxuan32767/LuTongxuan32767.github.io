$(function(){var a=isNewVisitor();if(a===true){$("body").pagewalkthrough({name:"introduction",steps:[{popup:{content:"#walkthrough-0",type:"modal"}},{wrapper:"#noviceBoot_1",popup:{content:"#walkthrough-1",type:"tooltip",position:"right"}},{wrapper:"#noviceBoot_2",popup:{content:"#walkthrough-2",type:"tooltip",position:"bottom"}},{wrapper:".noviceBoot_3",popup:{content:"#walkthrough-3",type:"tooltip",position:"bottom"}},{wrapper:"#noviceBoot_4",popup:{content:"#walkthrough-4",type:"tooltip",position:"left"}},{wrapper:"#noviceBoot_5",popup:{content:"#walkthrough-5",type:"tooltip",position:"left"}},{wrapper:"#noviceBoot_6",popup:{content:"#walkthrough-6",type:"tooltip",position:"top"}},{wrapper:"#noviceBoot_7",popup:{content:"#walkthrough-7",type:"tooltip",position:"top"}},{wrapper:"#noviceBoot_8",popup:{content:"#walkthrough-8",type:"tooltip",position:"bottom"}},{wrapper:".noviceBoot_9",popup:{content:"#walkthrough-9",type:"tooltip",position:"top"}}]});$("body").pagewalkthrough("show");setCookie("gznotes-visited","true",30)}});function isNewVisitor(){var a=getCookie("gznotes-visited");if(a===""){return true}else{return false}}function setCookie(b,f,c){var e=new Date();e.setTime(e.getTime()+(c*24*60*60*1000));var a="expires="+e.toUTCString();document.cookie=b+"="+f+"; "+a+";path=/"}function getCookie(d){var b=d+"=";var a=document.cookie.split(";");for(var e=0;e<a.length;e++){var f=a[e];while(f.charAt(0)==" "){f=f.substring(1)}if(f.indexOf(b)==0){return f.substring(b.length,f.length)}}return""};
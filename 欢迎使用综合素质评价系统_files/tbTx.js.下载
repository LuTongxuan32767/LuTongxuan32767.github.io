if(window.tbTx){alert("tbTx 冲突了！")}else{window.tbTx={}}$.extend(tbTx,{xn:null,xq:null,yhjs:null,obj:{tbTxText:ko.observable("")},setTbTxText:function(a){tbTx.obj.tbTxText(a)},clearTbTxText:function(){tbTx.obj.tbTxText("")},compareDate:function(d,c){if(!d||!c){E5.Msg.alert("比较的两个日期不能为空！");return}var b=parseInt(d);var a=parseInt(c);if(b>a){return 1}else{if(b==a){return 0}else{if(b<a){return -1}}}},calcDateJg:function(b,d){if(!b||!d){E5.Msg.alert("计算间隔的两个日期不能为空！");return}var a=tbTx.convertToDate(b);var c=tbTx.convertToDate(d);var e=parseInt(Math.abs(c.getTime()-a.getTime())/1000/60/60/24);return e},convertToDate:function(b){if(!b){E5.Msg.alert("日期不能为空！");return}if(b.length!=8){E5.Msg.alert("日期格式错误！");return}var a=new Date(b.substr(0,4),parseInt(b.substr(4,2))-1,b.substr(6,2));return a},isTx:function(e,f,d){if(!e||!f||!d){E5.Msg.alert("【提前提醒的天数】或者【公示天数】或者【最终截止时间】存在空值！");return false}var c=tbTx.getNowDate();var a=tbTx.calcDateJg(c,d);var b=parseInt(e)+f;if(a<=b){return true}else{return false}},calcSyTbts:function(d,c){if(!d||!c){E5.Msg.alert("【公示天数】或者【最终截止时间】不能为空！");return}var b=tbTx.getNowDate();var a=tbTx.calcDateJg(b,c);return a-d},getNowDate:function(){var c=new Date();var a=c.getFullYear();var f=c.getMonth()+1;var g=f+"";if(f<10){g="0"+f}var b=c.getDate();var e=b+"";if(b<10){e="0"+e}var d=a+g+e;return d},calcTbxx:function(m){if(m){var f=m.kssj;var i=m.jssj;var j=m.zxgsts;var c=m.gsjssj;var a=tbTx.isTx(TbTxConst.tqtxts,j,i);if(a&&tbTx.yhjs){var k=i;if(tbTx.yhjs=="134"&&TbTxConst.isGsxxq=="true"){k=c}var d=tbTx.calcSyTbts(j,k);var e=tbTx.xn+"学年";var b=CommonDict.get("DIC_ZD_XT_XQMC",tbTx.xq);var l=e+b+"的综合素质评价档案填写时间最多还有"+d+"天就截止了";if(tbTx.yhjs=="192"||tbTx.yhjs=="193"){l=l+"，请及时督促填写。"}else{if(tbTx.yhjs=="134"||tbTx.yhjs=="243"||tbTx.yhjs=="244"){l=l+"，请及时填写。"}}if(d<=0){l=e+b+"的综合素质评价档案填写已经截止了！"}tbTx.setTbTxText(l)}else{var h=tbTx.getNowDate();var g=tbTx.compareDate(h,f);tbTx.setTbTxText("");if(g<0){}else{}}}},queryTbTxAndShow:function(){var a=TbTxConst.uOrganCode;$.po
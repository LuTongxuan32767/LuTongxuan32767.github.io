if(window.jggsListWgt){alert("jggsListWgt 冲突了！")}else{window.jggsListWgt={}}$.extend(jggsListWgt,{obj:{content:ko.observableArray([])},queryData:function(){$.get(jggsListConst.jggsListAjaxUrl,{method:"queryJggsList",pageSize:7},function(returnData){jggsListWgt.obj.content.removeAll();if(returnData){var sfjs=returnData.sfjs;var gsjg=returnData.gsjg;if(sfjs=="true"||sfjs==true){$("#sfjsId").show();$("#gsjgId").hide();$("#jggsIds").hide()}else{if(gsjg=="false"||gsjg==false){$("#sfjsId").hide();$("#gsjgId").show();$("#jggsIds").hide()}else{var dat=returnData.jds;$.each(dat.rows,function(i,item){var sj=item.lrsj;if(sj!=null&&sj!=""&&sj.length>8){sj=sj.substr(0,4)+"/"+sj.substr(4,2)+"/"+sj.substr(6,2)}var pid={fbszId:item.fbszId,zzjgdm:item.zzjgdm,title:item.gsfbGsbt+"("+item.gsfbStart+"-"+item.gsfbEnd+")",gsfbGsbt:item.gsfbGsbt,gsfbGsfw:item.gsfbGsfw,gsfbGsfs:item.gsfbGsfs,tbjd:item.tbjd,lrsj:sj};jggsListWgt.obj.content.push(pid)})}}}}).error(function(msg){var error=eval("("+msg.responseText+")");E5.Msg.alert(error.message)})},clickjggs:function(c,b,a){if(a==jggsListConst.gsjc){$("#myModal").modal("show");jggsDetail.queryJggs(c)}else{$("#myModalXs").modal("show");jggsXsListConst.fbszId=c;jggsXsListConst.gsfbGsfw=b;jggsXsList.isSentAjaxFlag=true;jggsXsList.queryData()}},queryGsDack:function(a){$("#myModalGsDack").modal("show");xsGsDack.queryGsDetail(a)},queryMoreDat:function(){$("#myLiModal").modal("show");jggsList.isSentAjaxFlag=true;jggsList.queryData()}});$(function(){var a=document.getElementById("koJggsBt");if(a){ko.applyBindings(jggsListWgt.obj,a)}});